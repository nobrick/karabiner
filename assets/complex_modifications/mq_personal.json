{
  "title": "MQ Personal v1.0",
  "rules": [
    {
      "description": "Set CapsLock to ESC (pressed alone) and Ctrl (otherwise)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "command", "control", "option", "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Fn-CapsLock to CapsLock",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Fn-h/j/k/l to Left/Down/Up/Right (allowing any modifiers)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Left Opt-h/j/k/l to Left/Down/Up/Right (allowing any modifiers)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Fn-a to Home, Fn-e to End (allowing any modifiers)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Left Opt-a to Home, -e to End (allowing Shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Left Ctrl-v to PgDn, Opt-v to PgUp (allowing Shift, not in Vim)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "conditions": [{
            "type": "frontmost_application_unless",
            "bundle_identifiers": ["^org\\.vim\\.MacVim$"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "conditions": [{
            "type": "frontmost_application_unless",
            "bundle_identifiers": ["^org\\.vim\\.MacVim$"]
          }]
        }
      ]
    },
    {
      "description": "Set Left Opt-f/b to move forward/backward a word (allowing Shift, not in Vim, Terminal, iTerm2, Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "conditions": [{
            "type": "frontmost_application_unless",
            "bundle_identifiers": [
              "^org\\.vim\\.MacVim$",
              "^com\\.apple\\.Terminal$",
              "^com\\.googlecode\\.iterm2$",
              "^com\\.apple\\.finder$"
            ]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "conditions": [{
            "type": "frontmost_application_unless",
            "bundle_identifiers": [
              "^org\\.vim\\.MacVim$",
              "^com\\.apple\\.Terminal$",
              "^com\\.googlecode\\.iterm2$",
              "^com\\.apple\\.finder$"
            ]
          }]
        }
      ]
    },
    {
      "description": "Set Left Ctrl to ESC (pressed alone)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Fn (pressed alone) to Cmd-h",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "fn",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "h",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Set Left Cmd-h to Backspace (allowing no modifiers)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ]
        }
      ]
    },
    {
      "description": "Set Left Cmd (pressed alone) to open Quicksilver",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "0",
              "modifiers": [
                "left_command", "left_option", "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Set Right Cmd (pressed alone) to show OmniFocus quick entry",
      "manipulators": [
        {
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f",
              "modifiers": [
                "left_command",
                "left_option",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "MacVim: Set Left Ctrl-i to Ctrl-\\",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "backslash",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "conditions": [{
            "type": "frontmost_application_if",
            "bundle_identifiers": ["^org\\.vim\\.MacVim$"]
          }]
        }
      ]
    },
    {
      "description": "MacVim: Set Cmd-p to Cmd-b",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "b",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "conditions": [{
            "type": "frontmost_application_if",
            "bundle_identifiers": ["^org\\.vim\\.MacVim$"]
          }]
        }
      ]
    },
    {
      "description": "Video apps: Set tab to toggle navigation, h/l and a/d to navigate",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "no_video_navigation",
                "value": 1
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"Video Navigation (h/l, a/d) Disabled\" with title \"Navigation OFF\"'"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "no_video_navigation",
                "value": 0
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"Video Navigation (h/l, a/d) Enabled\" with title \"Navigation ON\"'"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "no_video_navigation",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^tv\\.sohu\\.SHPlayer$", "^com\\.tencent\\.tenvideo$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
